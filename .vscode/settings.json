        "*.module.ts": "$(capture).entity.ts, $(capture).service.ts, $(capture).controller.ts",
        "*.js": "",
        "package.json": "esb.dev.mjs, .gitignore, svelte.config.js, postcss.*, package-lock.*, tailwind.*, tsconfig.*, .env"
    },
    "material-icon-theme.files.associations": {
